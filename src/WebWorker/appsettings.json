{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MaxWorkers": 400,
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "Port": 5672,
    "AutomaticRecoveryEnabled": true,
    "Channel": {
      "ExchangeType": "direct",
      "Qos": {
        "PrefetchSize": 0,
        "PrefetchCount": 1,
        "Global": false
      }
    },
    "Queue": {
      "AutoAck": false,
      "Durable": true,
      "Exclusive": false,
      "AutoDelete": false,
      "Arguments": {
        "x-single-active-consumer": true
      }
    }
  }
}
